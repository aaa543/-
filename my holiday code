class Slution:
    def reverse(self,x):
        """
        :type x:int
        :r type:int
        """
        if x==0:
            return 0
        str_x=str(x)
        x=''
        if str_x[0]=='-':
            x+='-'
            x+=str_x[len(str_x)-1::-1].lstrip("0").rstrip("-")
            x=int(x)
        if -2**31<x<2**31-1:
            return x
        else:
            return 0
        s=Slution()
        print(s)
